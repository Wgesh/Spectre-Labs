    if movement = 1 {
        direction = 180;
        
        if !place_meeting(x-32,y,obj_metal_ground) {
            slidetimer = 0;
            alarm[0] = 1;
            
            movement = 0;
            movetimer = 16;
        }
    }
}

if place_meeting(x,y-2,obj_player) {
    if movement = 1 {
        direction = 270;
        
        if !place_meeting(x,y+32,obj_metal_ground) {
            slidetimer = 0;
            alarm[0] = 1;
            
            movement = 0;
            movetimer = 16;
        }
    }
}

