            <string>/// player push checking start ///

// checks for meeting obj_player and not meeting obj_metal_ground in next position //
// sets alarm, slidetimer, movement, movetimer //
if place_meeting(x-2,y,obj_player) {
    if movement = 1 {
        direction = 0;
        
        if !place_meeting(x+32,y,obj_metal_ground) {
            slidetimer = 0;
            alarm[0] = 1;
            
            movement = 0;
            movetimer = 16;
        }
    }
}

