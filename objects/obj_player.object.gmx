          <argument>
            <kind>1</kind>
            <string>/// keyboard movement checking ///

// checks for keyboard input and other parameters to move the player //
// sets alarm, slidetimer, movement, movetimer //
if global.movecontrollock = 1 {
    if keyboard_check(ord('A')) {
        if movement = 1 {
            direction = 180;
    
            if !place_meeting(x-32,y,obj_metal_wall) 
            and !place_meeting(x-32,y,obj_metal_ground_oneway_left) {
            
                slidetimer = 0;
                alarm[0] = 1;
    
                movement = 0;
                movetimer = 16;
            }
        } 
    }

    if keyboard_check(ord('D')) {
        if movement = 1 {
            direction = 0;
        
            if !place_meeting(x+32,y,obj_metal_wall) 
            and !place_meeting(x+32,y,obj_metal_ground_oneway_right) { 
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }

    if keyboard_check(ord('W')) {
        if movement = 1 {
            direction = 90;
        
            if !place_meeting(x,y-32,obj_metal_wall) 
            and !place_meeting(x,y-32,obj_exit_door_right_green) 
            and !place_meeting(x,y-32,obj_exit_door_left_green) 
            and !place_meeting(x,y-32,obj_exit_door_right_blue)
            and !place_meeting(x,y-32,obj_exit_door_left_blue)
            and !place_meeting(x,y-32,obj_metal_ground_oneway_top) {
                
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }

    if keyboard_check(ord('S')) {
        if movement = 1 {
            direction = 270;
        
            if !place_meeting(x,y+32,obj_metal_wall) 
            and !place_meeting(x,y+32,obj_exit_door_right_green) 
            and !place_meeting(x,y+32,obj_exit_door_left_green) 
            and !place_meeting(x,y+32,obj_exit_door_right_blue)
            and !place_meeting(x,y+32,obj_exit_door_left_blue)
            and !place_meeting(x,y+32,obj_metal_ground_oneway_bot) {
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }
}

/// keyboard movement checking end ///
/// movement checking for push block held up against wall ///


</string>
