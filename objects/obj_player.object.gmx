            <string>/// level completion start ///

// if player past top of room, according to room, act accordingly //
if place_meeting(x,y,obj_level_complete) {
    if room = /*room_tutorial*/ room_feature_test {
        global.fadein = 1;
        instance_create(0,0,obj_fade);
    }
    if room = room_level1 {
        global.fadein = 1;
        instance_create(0,0,obj_fade);    
