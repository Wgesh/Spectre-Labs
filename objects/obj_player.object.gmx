            <string>/// keyboard movement checking ///
if global.movecontrollock = 1{
    if keyboard_check(ord('A')) {
        if movement = 1 {
            direction = 180;
    
            if !place_meeting(x-32,y,obj_metal_wall){
            
                slidetimer = 0;
                alarm[0] = 1;
    
                movement = 0;
                movetimer = 16;
            }
        } 
    }

    if keyboard_check(ord('D')) {
        if movement = 1 {
            direction = 0;
        
            if !place_meeting(x+32,y,obj_metal_wall) { 
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }

    if keyboard_check(ord('W')) {
        if movement = 1 {
            direction = 90;
        
            if !place_meeting(x,y-32,obj_metal_wall) and !place_meeting(x,y-32,obj_exit_door_right) and !place_meeting(x,y-32,obj_exit_door_left) {
                
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }

    if keyboard_check(ord('S')) {
        if movement = 1 {
            direction = 270;
        
            if !place_meeting(x,y+32,obj_metal_wall) {
                slidetimer = 0;
                alarm[0] = 1;
        
                movement = 0;
                movetimer = 16;
            }
        }
    }
}
