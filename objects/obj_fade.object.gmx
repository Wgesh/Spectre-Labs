          <argument>
            <kind>1</kind>
            <string>/// fading effect start ///

// if fadein state, decrease image_alpha, increase fadetimer //
if global.fadein = 0 {
    image_alpha -= 0.04;
    fadetimer += 1;
    
    // if fadetimer &lt; 25, restart alarm, else destroy self, unlock global.movecontrollock //
    if fadetimer &lt; 25 {
        alarm[0] = 1;
    } else {
        if room != room_boss {
            global.movecontrollock = false;
        }
        instance_destroy();
    }
}

// if fadeout state, incrase image_alpha, increase fadetimer //
if global.fadein = 1 {
    image_alpha += 0.04;
    fadetimer += 1;
    
    // if fadetimer &lt; 25, restart alarm //
    if fadetimer &lt; 25 {
    alarm[0] = 1;
    
    // else choose room depending on current room and global.startselection //
    } else {
    
        if room = room_feature_test {
            room_goto(room_title_screen);
        }
        
        if room = room_tutorial_old {
            room_goto(room_title_screen);
        }
        
        if room = room_level1 {
            room_goto(room_level2);
        }
        
        if room = room_level2 {
            room_goto(room_level3);
        }
        
        if room = room_level3 {
            room_goto(room_control_room);
        }
        
        if room = room_control_room {
            room_goto(room_vent);
        }
        
        if room = room_vent {
            room_goto(room_boss);
        }
        
        if room = room_title_screen {
            if global.startselection = 0 {
                room_goto(/*room_start_cut*/ room_level1);
            }
            if global.startselection = 1 {
                room_goto(room_feature_test /*room_boss*/);
            }
        }
        
    }
}

/// fading effect end ///
</string>
